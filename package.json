{
  "name": "hygen-cook",
  "version": "1.4.0",
  "description": "Helps you cook up some delicious hygen recipes",
  "main": "dist/main/chef.cjs",
  "bin": {
    "hygen-cook": "dist/bin/hygen-chef.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava",
    "pretest": "npm run lint && npm run compile",
    "lint": "eslint .",
    "precompile": "rm -rf generators/* || true",
    "compile:cjs": "rm -rf dist || true && babel src --out-dir dist --extensions '.ts,.cjs,.mjs' --copy-files --include-dotfiles --no-copy-ignored --ignore '**/*.test.ts' --out-file-extension '.cjs' --config-file ./babel.config.cjs",
    "compile": "npm run compile:cjs && tsc --emitDeclarationOnly",
    "release": "semantic-release"
  },
  "author": "Bill Beesley <bill@beesley.dev>",
  "license": "MIT",
  "optionalDependencies": {
    "fsevents": "2.3.2"
  },
  "dependencies": {
    "async-fs-wrapper": "^2.7.0",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "exec-sh": "^0.4.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.1",
    "hygen": "^6.1.5",
    "hygen-add": "^1.0.5",
    "inquirer": "^8.2.0",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1",
    "p-limit": "^3.1.0",
    "yargs": "^17.3.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.14",
    "@types/sinon": "^10.0.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "ava": "^4.0.1",
    "babel-plugin-add-import-extension": "^1.6.0",
    "conventional-changelog-angular": "^5.0.13",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^13.2.5",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "hygen-emiketic-react": "github:emiketic/hygen-emiketic-react",
    "hygen-react-ts": "^1.0.1",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "sinon": "^13.0.1",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps*",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "README.md"
          ],
          "message": "chore(release): ${nextRelease.version} ðŸš€ [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
